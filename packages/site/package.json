    "qitmeerts": "^1.1.2",
    "randombytes": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "stream-browserify": "^3.0.0",
    "styled-components": "5.3.3",
    "tiny-secp256k1": "^1.1.6",
